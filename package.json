{
  "name": "@harmowatch/redux-decorators",
  "version": "0.0.5",
  "main": "./lib/index.js",
  "types": "./lib/index",
  "files": [
    "lib/"
  ],
  "scripts": {
    "test": "karma start karma.conf.js && npm run coveralls",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "coveralls": "cat ./coverage/coverage.lcov | node node_modules/.bin/coveralls",
    "package:build": "rimraf ./lib && tsc",
    "package:publish:public": "npm run package:build && npm publish --access=public --registry https://registry.npmjs.org",
    "package:publish:public:next": "npm run package:build && npm publish --access=public --tag=next --registry https://registry.npmjs.org",
    "package:publish:private": "npm run package:build && npm publish --registry https://npm.harmowatch.com --force"
  },
  "license": "MIT",
  "peerDependencies": {
    "rxjs": "5.x"
  },
  "devDependencies": {
    "@types/jasmine": "2.8.6",
    "babel-core": "6.26.0",
    "coveralls": "3.0.0",
    "cross-env": "5.1.3",
    "jasmine": "3.0.0",
    "jasmine-core": "3.0.0",
    "karma": "2.0.0",
    "karma-babel-preprocessor": "7.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.1.0",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-typescript": "3.0.12",
    "reflect-metadata": "0.1.12",
    "rimraf": "2.6.2",
    "rxjs": "5.5.6",
    "tslint": "5.9.1",
    "tslint-no-circular-imports": "0.2.1",
    "typescript": "2.7.1"
  }
}
